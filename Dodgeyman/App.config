<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="GeneralFontLocation" value="Assets/5x5all.png"/>
    <!-- number font doesn't include pixels under the numbers. it's easier to center. -->
    <add key="NumberFontLocation" value="Assets/5x5numbers.png"/>
    <!-- What percent of line spawns will be patterns, as opposed to single lines. 0-100 -->
    <add key="PatternPercentage" value="30"/>
    <!-- What the dimensions of the screen are (the screen is square, deal with it). -->
    <add key="ScreenSize" value="600"/>
    <add key="VersionNumber" value="0.1"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="lib"/>
    </assemblyBinding>
  </runtime>
</configuration>